{
    "name": "cttapi",
    "version": "0.0.1",
    "description": "Awesome project developed with TypeORM.",
    "type": "commonjs",
    "devDependencies": {
        "@types/node": "^16.11.10",
        "ts-node": "10.7.0",
        "ts-node-dev": "^2.0.0",
        "tscpaths": "^0.0.9",
        "typescript": "4.5.2"
    },
    "dependencies": {
        "@types/express": "^4.17.13",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.17.2",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.4",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "0.3.9",
        "uuid": "^9.0.0"
    },
    "scripts": {
        "start:prod": "ts-node src/shared/index.ts",
        "start:dev": "ts-node-dev src/shared/index.ts",
        "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
        "typeorm:create": "typeorm-ts-node-commonjs migration:create",
        "typeorm:run": "typeorm-ts-node-commonjs migration:run -d src/shared/database/data-source.ts"
    }
}